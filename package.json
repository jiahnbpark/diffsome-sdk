{
  "name": "@diffsome/sdk",
  "version": "3.2.7",
  "description": "Diffsome SDK for JavaScript/TypeScript - Different + Awesome",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "generate:types": "openapi-typescript $(grep '^APP_URL=' ../../.env | cut -d '=' -f2)/docs/api.json -o src/types/generated.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "diffsome",
    "cms",
    "headless",
    "api",
    "sdk"
  ],
  "author": "jiahnpark",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/diffsome/sdk.git"
  },
  "devDependencies": {
    "openapi-typescript": "^7.10.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
